@lt-gray: #B0BEC5;
@orange: #DD661E;
@blue: #0021A5;
@dark-blue: #014A83;

.p {
	color: @lt-gray;
	font-size: medium;
	font-weight: 400;
}

html {
	width: 100%;
	height: 100%;
	position: relative;
	font-family: "Open Sans", Arial, sans-serif;
	background-color: @dark-blue;

	> body {
		height: 100%;
		margin: 0px;
		font-family: inherit;
	}
}

.jumbotron {
	background-color: transparent;
	h1 {
		text-align: center;
		&.title {
			font-family: 'Lato', Arial, sans-serif;
			font-weight: 900;
			color: white;
		}
		&.subtitle {
			color: @orange;
			text-align: center;
			font-family: 'Oswald', Arial, sans-serif;
			font-weight: 300;
		}
	}
	h2 {
		color: @orange;
		font-family: 'Oswald', Arial, sans-serif;
		font-weight: 300;
		text-align: center;
	}
	table {
		margin: 0 auto;
		color: @orange;
		th {
			font-family: 'Oswald', Arial, sans-serif;
			font-weight: 300;
			text-align: center;
		}
	}
	p, span {
		.p();
		text-align: center;
		&.phone {
			text-decoration: none;
		}
	}
	td {
		.p();
		padding: 0px 15px 0px 0px;
	}

	a {
		color: white;
		&:link {
			color: white;
		}
		&:visited {
			color: white;
		}
		&:hover {
			color: white;
		}
		&:active {
			color: white;
		}
	}
}

#page-wrap {
	background-color: @dark-blue;
	height: 100%;
	width: 100%;
}

#content {
	width: 100%;
	height: 100%;
}

.maps {
	display: block;
	margin: 0 auto;
}

